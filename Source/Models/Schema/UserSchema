const mongoose=require('mongoose');

const userSchema=new mongoose.Schema(
    {
        userId:{
            type:String,
            unique:true,
            required:true
        },
        name:{
            type:String,
            unique:false,
            required:false
        },
        emailId:{
            type:String,
            unique:true,
            required:true,
        },
        phoneNo:{
            type:Number,
            required:false,
        }
    }
)
const UserModel=mongoose.model('user',userSchema);
module.exports=UserModel;